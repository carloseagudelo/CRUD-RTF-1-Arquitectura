<p id="notice"><%= notice %></p>

<h1>Listing Socios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Document</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>Password</th>
      <th>Numberpoints</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.name %></td>
        <td><%= socio.document %></td>
        <td><%= socio.lastname %></td>
        <td><%= socio.email %></td>
        <td><%= socio.password %></td>
        <td><%= socio.numberpoints %></td>
        <td><%= link_to 'Show', socio %></td>
        <td><%= link_to 'Edit', edit_socio_path(socio) %></td>
        <td><%= link_to 'Destroy', socio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Socio', new_socio_path %>
